{
  "name": "@salable/errors",
  "version": "1.0.3",
  "description": "HTTPException classes for specific error messages in response to HTTP requests to improve error handling in an application.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "author": "Salable",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest - --watch",
    "test:globalCoverage": "jest --coverage --passWithNoTests",
    "test:coverage": "jest --coverage --changedSince=origin/main --passWithNoTests",
    "build": "rollup -c",
    "preversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Salable/errors.git"
  },
  "keywords": [
    "salable",
    "errors",
    "HTTP",
    "HTTP",
    "requests",
    "HTTPException",
    "error",
    "handling",
    "error",
    "messages"
  ],
  "bugs": {
    "url": "https://github.com/Salable/errors/issues"
  },
  "homepage": "https://github.com/Salable/errors#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^21.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
